@model IEnumerable<OrderViewModel>

@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Orders</h1>
<div class="row">
    <div class="col">
        @foreach (var order in Model)
        {
            @await Html.PartialAsync("_OrderHorizontalCardPartialView", order);
        }
    </div>
</div>

